#!/bin/bash
#===============================================================================
# Services
#===============================================================================

#-------------------------------------------------------------------------------
# create services
#-------------------------------------------------------------------------------
yaneCreateServices () {
   for serv in ${serviceName[@]}
   do
      createService_${serviceType[$serv]} $serv
   done
}

#-------------------------------------------------------------------------------
# shutdown services
#-------------------------------------------------------------------------------
yaneShutdownServices () {
   for serv in ${serviceName[@]}
   do
      shutdownService_${serviceType[$serv]} $serv
   done
}

#-------------------------------------------------------------------------------
# delete services
#-------------------------------------------------------------------------------
yaneDeleteServices () {
   for serv in ${serviceName[@]}
   do
      deleteService_${serviceType[$serv]} $serv
   done
}
